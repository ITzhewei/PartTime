<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.zzw.john.parttime"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name=".base.MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
        </activity>
        <activity
            android:name=".model.login.LoginActivity"
            android:label="Login In">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".model.register.RegisterActivity"
            android:label="Register">
        </activity>
        <activity android:name=".model.index.AllJobActivity">
        </activity>
        <activity android:name=".model.me.ResumeActivity"  android:windowSoftInputMode="stateHidden"/>
        <activity android:name=".model.me.EnrollActivitiy" android:windowSoftInputMode="stateHidden">

        </activity>
    </application>

</manifest>